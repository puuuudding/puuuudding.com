{
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "targets": "last 2 versions, Firefox ESR, > 1%, ie >= 11",
      "corejs": "3.6",
      "modules": false
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-proposal-decorators", false],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
